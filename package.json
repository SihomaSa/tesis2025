{
  "name": "unmsm-sentiment-analysis",
  "version": "3.0.0",
  "description": "Sistema Completo de Análisis de Sentimientos - UNMSM",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd FRONTEND && npm install && ng build --configuration production",
    "build:backend": "cd BACKEND && pip install -r requirements.txt",
    "railway:build": "npm run build:frontend && cd BACKEND && pip install -r requirements.txt",
    "railway:start": "node server.js",
    "test": "echo 'No tests yet'",
    "verify": "node verify-deployment.js"
  },
  "keywords": [
    "sentiment-analysis",
    "nlp",
    "machine-learning",
    "fastapi",
    "express",
    "unmsm",
    "instagram"
  ],
  "author": "UNMSM - Facultad de Ingeniería de Sistemas",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0",
    "python": ">=3.12.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SihomaSa/tesis2025.git"
  }
}